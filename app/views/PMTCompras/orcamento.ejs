<!DOCTYPE html>
<html>

<head>
    <title>Preco</title>
    <!-- novas dependencias FRONT -->
    <link rel="stylesheet" href="../source/bootstrap/css/bootstrap.min.css">
    <link rel="stylesheet" href="../source/plugins/iCheck/flat/blue.css">
    <link rel="stylesheet" href="../source/css/AdminLTE.min.css">
    <link rel="stylesheet" href="../source/css/font-awesome.min.css">
    <link rel="stylesheet" href="../source/css/ionicons.min.css">
    <link rel="stylesheet" href="../source/css/skins/_all-skins.min.css">
</head>

<body class="hold-transition skin-blue layout-top-nav">

    <div class="wrapper">

        <header class="main-header">
            <nav class="navbar navbar-static-top">
                <div class="container">
                    <div class="navbar-header">
                        <a href="#" class="logo">
                <span class="logo-mini">
                    <b>S</b>TM
                </span>
                <span class="logo-lg">
                    <b>S</b>tatisticum</span>
            </a>
                        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar-collapse">
            <i class="fa fa-bars"></i>
          </button>
                    </div>

                    <!-- Collect the nav links, forms, and other content for toggling -->
                    <div class="collapse navbar-collapse pull-right" id="navbar-collapse">
                        <ul class="nav navbar-nav">
                            <li><a href="/PMTCompras">voltar</a></li>
                        </ul>

                    </div>
                    <!-- /.navbar-collapse -->
                    <!-- /.navbar-custom-menu -->
                </div>
                <!-- /.container-fluid -->
            </nav>
        </header>
        <div class="content-wrapper">
            <div class="container">
                <!-- Content Header (Page header) -->
                <section class="content-header">
                    <h1>
                        Gerar Orçamento
                        <small>Tabela Oficial</small>
                    </h1>
                </section>
                <section class="content">
                    <div class="box box-primary">
                        <div class="box-header with-border">
                            <button class="btn btn-primary btn-lg pull-right" id="pdf">Gerar PDF</button>
                        </div>
                        <div class="box-body">
                            <table class="table table-bordered" id="tblData" border="1" cellpadding="2">
                                <thead>
                                    <tr id="headers">
                                        <th style="width: 250px">Descrição</th>
                                        <th>Região/UF</th>
                                        <th>Grupo</th>
                                        <th>Referência</th>
                                        <th>CNPJ/CPF</th>
                                        <th>Pregão</th>
                                        <th>Número de cotações</th>
                                        <th style="width: 100px">Preço</th>
                                        <th>Qtd. Compra</th>
                                        <th>Valor Total</th>
                                    </tr>
                                </thead>
                                <tbody id="tblbody">
                                    <% for(var i = 0; i < carrinho.length; i++){%>
                                        <tr>
                                            <td id="3">
                                                <%- carrinho[i].descricao.toLowerCase() %>
                                            </td>
                                            <td id="4">
                                                <%- carrinho[i].regiao %>
                                            </td>
                                            <td id="5">
                                                <%- carrinho[i].grupo %>
                                            </td>
                                            <td id="6">
                                                <%- carrinho[i].referencia %>
                                            </td>
                                            <td id="7">
                                                <%- carrinho[i].cnpj %>
                                            </td>
                                            <td id="8">
                                                <%- carrinho[i].pregao %>
                                            </td>
                                            <td id="9">
                                                <%- carrinho[i].cotacoes %>
                                            </td>
                                            <td id="10"> R$
                                                <%- carrinho[i].precoFinal %>
                                            </td>
                                            <td id="11">
                                                <%- carrinho[i].quantidade %>
                                            </td>
                                            <td id="12"> R$
                                                <%- carrinho[i].precoFinal * carrinho[i].quantidade %>
                                            </td>
                                        </tr>
                                        <%}%>
                                </tbody>
                            </table>
                        </div>
                        <!-- /.box-body -->
                        <div class="box-footer">
                            <h2 class="pull-right">Valor Total : R$
                                <%- valorTotal %>
                            </h2>
                        </div>
                    </div>

                </section>
            </div>
        </div>
    </div>

</body>

<script src="https://ajax.googleapis.com/ajax/libs/jquery/3.2.1/jquery.min.js"></script>
<!-- SlimScroll -->
<script src="../source/plugins/slimScroll/jquery.slimscroll.min.js"></script>
<!-- FastClick -->
<script src="../source/plugins/fastclick/fastclick.min.js"></script>
<!-- AdminLTE App -->
<script src="../source/js/adminlte.min.js"></script>
<!-- Slider -->
<script src="../source/plugins/jQueryUI/jquery-ui.min.js"></script>
<!-- AdminLTE for demo purposes -->
<script src="../source/js/demo.js"></script>
<script>
    $(document).ready(function() {
        $('.sidebar-menu').tree()
    })

</script>
<script src="https://unpkg.com/jspdf@latest/dist/jspdf.min.js"></script>
<script src="https://cdnjs.cloudflare.com/ajax/libs/jspdf-autotable/2.3.2/jspdf.plugin.autotable.js"></script>
<script src="/javascripts/PMTCompras/orcamento.js"></script>

</html>
